{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "mkdir public/build && npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/build/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/build/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@material/elevation": "^12.0.0",
    "@material/typography": "^13.0.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@smui/button": "^4.2.0",
    "@smui/card": "^4.2.0",
    "@smui/dialog": "^4.2.0",
    "@smui/drawer": "^4.2.0",
    "@smui/fab": "^4.2.0",
    "@smui/icon-button": "^4.2.0",
    "@smui/list": "^4.2.0",
    "@smui/select": "^4.2.0",
    "@smui/slider": "^4.2.0",
    "@smui/snackbar": "^4.2.0",
    "@smui/tab": "^4.2.0",
    "@smui/tab-bar": "^4.2.0",
    "@smui/textfield": "^4.2.0",
    "@smui/top-app-bar": "^4.2.0",
    "@tsconfig/svelte": "^2.0.1",
    "@types/node-sass": "^4.11.2",
    "node-sass": "^6.0.1",
    "openapi-typescript-codegen": "^0.13.0",
    "protobuf-typescript": "^6.8.8",
    "protobufjs": "^6.11.2",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "rxjs": "^7.3.0",
    "sass": "~1.39.2",
    "smui-theme": "^4.2.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.3.0",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.0.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@microsoft/signalr": "^6.0.1",
    "axios": "^0.24.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sirv-cli": "^1.0.0",
    "svelte-material-ui": "^4.2.0",
    "ts-proto": "^1.82.5",
    "websocket-ts": "^1.1.1"
  }
}
